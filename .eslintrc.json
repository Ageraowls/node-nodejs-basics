{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-unused-vars": "warn",
    "no-restricted-syntax": "off"
  }
}
